        android:supportsRtl="true"
        android:theme="@style/Theme.Shoppy_isra">
        <activity
            android:name="com.tugasday1.shoppy_isra.activity.LoginActivity"
            android:label="Shoppy_isra"
            android:theme="@style/Theme.Shoppy_isra.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tugasday1.shoppy_isra.activity.MainActivity"
            android:theme="@style/Theme.Shoppy_isra.NoActionBar"/>
    </application>

</manifest>
